- {
-+	rtl8xxxu_write8(priv, REG_RF_CTRL, RF_ENABLE | RF_RSTB | RF_SDMRSTB);
-+
- 	rtl8xxxu_write8(priv, REG_TXPAUSE, 0x00);
- }
