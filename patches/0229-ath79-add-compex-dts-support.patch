Index: build_dir/target/linux/ath79/dts/qca9558_compex_wpj558.dts
===================================================================
--- /dev/null
+++ build_dir/target/linux/ath79/dts/qca9558_compex_wpj558.dts
@@ -0,0 +1,152 @@
+// SPDX-License-Identifier: GPL-2.0-or-later OR MIT
+/dts-v1/;
+
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/input/input.h>
+
+#include "qca9557.dtsi"
+
+/ {
+	compatible = "compex,wpj558", "qca,qca9558";
+	model = "Compex WPJ558";
+
+        chosen {
+                bootargs = "console=ttyS0,115200n8";
+        };
+
+	extosc: ref {
+		compatible = "fixed-clock";
+		#clock-cells = <0>;
+		clock-output-names = "ref";
+		clock-frequency = <40000000>;
+	};
+
+	leds {
+		compatible = "gpio-leds";
+
+		wifi_red {
+			label = "wpj344:red:sig1";
+			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
+		};
+
+		wifi_yellow {
+			label = "wpj344:yellow:sig2";
+			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
+		};
+
+		wifi_green {
+			label = "wpj344:green:sig3";
+			gpios = <&gpio 22 GPIO_ACTIVE_LOW>;
+		};
+
+		wifi_green2 {
+			label = "wpj344:green:sig4";
+			gpios = <&gpio 23 GPIO_ACTIVE_LOW>;
+		};
+
+		buzzer {
+			label = "wpj344:buzzer";
+			gpios = <&gpio 4 GPIO_ACTIVE_HIGH>;
+		};
+	};
+
+	keys {
+		compatible = "gpio-keys-polled";
+		poll-interval = <20>;
+
+		button0 {
+			label = "reset";
+			linux,code = <KEY_RESTART>;
+			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
+                        debounce-interval = <60>;
+		};
+	};
+};
+
+&pcie0 {
+	status = "okay";
+};
+
+&uart {
+	status = "okay";
+};
+
+&pll {
+	clocks = <&extosc>;
+};
+
+&spi {
+	status = "okay";
+	num-cs = <1>;
+
+	flash@0 {
+		compatible = "jedec,spi-nor";
+		reg = <0>;
+		spi-max-frequency = <25000000>;
+
+		partitions {
+			compatible = "fixed-partitions";
+			#address-cells = <1>;
+			#size-cells = <1>;
+
+			uboot: u-boot@0 {
+				label = "u-boot";
+				reg = <0x000000 0x030000>;
+				read-only;
+			};
+
+			firmware@30000 {
+				compatible = "denx,uimage";
+				label = "firmware";
+				reg = <0x030000 0xfc0000>;
+			};
+
+			art: art@ff0000 {
+				label = "ART";
+				reg = <0xff0000 0x010000>;
+				read-only;
+			};
+		};
+	};
+};
+
+&mdio0 {
+	status = "okay";
+
+        phy0: ethernet-phy@0 {
+                reg = <0>;
+
+                qca,ar8327-initvals = <
+                        0x04 0x00080080 /* PORT0 PAD MODE CTRL */
+                        0x0c 0x07600000 /* PORT6 PAD MODE CTRL */
+                        0x10 0x81000080 /* POWER_ON_STRAP */
+                        0x50 0xc737c737 /* LED_CTRL0 */
+                        0x54 0xc737c737 /* LED_CTRL1 */
+                        0x58 0xc737c737 /* LED_CTRL2 */
+                        0x5c 0x3ffff000 /* LED_CTRL3 */
+                        0x7c 0x0000007e /* PORT0_STATUS */
+                        0x94 0x0000007e /* PORT6 STATUS */
+                        >;
+        };
+};
+
+&eth0 {
+        compatible = "qca,qca9550-eth", "syscon";
+	status = "okay";
+
+        mtd-mac-address = <&uboot 0x2e010>;
+
+	phy-handle = <&phy0>;
+        pll-data = <0x56000000 0x00000101 0x00001616>;
+        gmac-config {
+                device = <&gmac>;
+                rgmii-enabled = <1>;
+        };
+};
+
+&wmac {
+        status = "okay";
+        mtd-cal-data = <&art 0x1000>;
+        mtd-mac-address = <&uboot 0x2e010>;
+        mtd-mac-address-increment = <3>;
+};
